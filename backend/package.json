  "scripts": {
    "test": "mocha -r ts-node/esm -r tsconfig-paths/register \"tests/**/*.ts\"",
    "docker:start": "docker compose up -d",
    "docker:stop": "docker compose stop",
    "docker:down": "docker compose down",
    "docker:local-stack:start": "docker compose -f local-stack-compose.yml up -d",
    "docker:local-stack:stop": "docker compose -f local-stack-compose.yml stop",
    "docker:local-stack:down": "docker compose -f local-stack-compose.yml down",
    "queues:prod": "node ./dist/scripts/queues.js",
    "queues:dev": "ts-node -r tsconfig-paths/register --transpile-only ./src/scripts/queues.ts",
    "build": "npx tsc --build & copyfiles -f src/services/MailService/templates/*.mustache dist/services/MailService/templates",
    "prod": "node -r ./prod-paths.js dist/main.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/main.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:extension": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli/index.js",
    "db:seed": "npm run typeorm:extension -- -d ./src/config/data-source/index.ts seed",
    "db:drop": "npm run typeorm:extension -- -d ./src/config/data-source/index.ts db:drop",
    "db:create": "npm run typeorm:extension -- -d ./src/config/data-source/index.ts db:create",
    "db:update:permissions": "npx tsc & node ./dist/scripts/reflect-permissions.js",
    "db:recreate:dev": "npm run db:drop && npm run db:create && npm run db:seed",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/config/data-source/index.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./src/config/data-source/index.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/config/data-source/index.ts",
    "docker:db": "docker exec -it db bash",
    "docker:redis": "docker exec -it redis bash",
    "eslint:fix": "npx eslint --fix ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.485.0",
