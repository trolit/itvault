{
  "name": "itvault-backend",
  "version": "0.0.0",
  "description": "",
  "main": "main.js",
  "keywords": [],
  "author": "https://github.com/trolit",
  "license": "ISC",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/main.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/main.js\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "db:seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli/index.js -d ./src/config/data-source/index.ts seed",
    "db:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli/index.js -d ./src/config/data-source/index.ts db:drop",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/config/data-source/index.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./src/config/data-source/index.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/config/data-source/index.ts",
    "docker:db": "docker exec -it db bash"
  },
  "_moduleAliases": {
    "@dtos": "dist/dtos",
    "@enums": "dist/enums",
    "@config": "dist/config",
    "@routes": "dist/routes",
    "@helpers": "dist/helpers",
    "@schemas": "dist/schemas",
    "@entities": "dist/entities",
    "@factories": "dist/factories",
    "@utilities": "dist/utilities",
    "@interfaces": "dist/interfaces",
    "@migrations": "dist/migrations",
    "@controllers": "dist/controllers",
    "@middlewares": "dist/middlewares",
    "@repositories": "dist/repositories"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "env-var": "^7.3.0",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.10",
    "typeorm-extension": "^2.1.15",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "concurrently": "^7.5.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
