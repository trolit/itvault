{
  "name": "itvault-backend",
  "version": "0.0.0",
  "description": "",
  "main": "main.js",
  "keywords": [],
  "author": "https://github.com/trolit",
  "license": "ISC",
  "scripts": {
    "dir:dist": "cross-env WORKING_DIRECTORY=dist",
    "prod": "node dist/main.js",
    "dev": "npm run dir:dist concurrently \"npx tsc --watch\" \"nodemon -q dist/main.js\"",
    "build": "npx tsc & copyfiles -f src/services/MailService/templates/*.mustache dist/services/MailService/templates",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:extension": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli/index.js",
    "db:seed": "npm run typeorm:extension -- -d ./src/config/data-source/index.ts seed",
    "db:drop": "npm run typeorm:extension -- -d ./src/config/data-source/index.ts db:drop",
    "db:create": "npm run typeorm:extension -- -d ./src/config/data-source/index.ts db:create",
    "db:update:permissions": "npx tsc & node ./dist/scripts/reflect-permissions.js",
    "db:recreate:dev": "npm run db:drop && npm run db:create && npm run db:seed",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/config/data-source/index.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./src/config/data-source/index.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/config/data-source/index.ts",
    "docker:init": "docker compose up -d",
    "docker:db": "docker exec -it db bash",
    "docker:redis": "docker exec -it redis bash",
    "eslint:fix": "npx eslint --fix ."
  },
  "_moduleAliases": {
    "types": "dist/types",
    "@jobs": "dist/jobs",
    "@dtos": "dist/types/dtos",
    "@enums": "dist/types/enums",
    "@interfaces": "dist/types/interfaces",
    "@utils": "dist/utils",
    "@config": "dist/config",
    "@routes": "dist/routes",
    "@helpers": "dist/helpers",
    "@schemas": "dist/schemas",
    "@entities": "dist/entities",
    "@services": "dist/services",
    "@factories": "dist/factories",
    "@middleware": "dist/middleware",
    "@migrations": "dist/migrations",
    "@controllers": "dist/controllers",
    "@repositories": "dist/repositories",
    "@consumer-handlers": "dist/consumer-handlers"
  },
  "dependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/formidable": "^2.0.6",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.194",
    "@types/module-alias": "^2.0.1",
    "@types/mustache": "^4.2.2",
    "@types/node": "^18.11.0",
    "@types/nodemailer": "^6.4.8",
    "@types/sanitize-html": "^2.9.0",
    "amqplib": "^0.10.3",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^2.4.0",
    "dayjs": "^1.11.8",
    "dotenv": "^16.0.3",
    "env-var": "^7.3.0",
    "express": "^4.18.1",
    "formidable": "^2.1.1",
    "fs-extra": "^11.1.1",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mustache": "^4.2.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.9.3",
    "reflect-metadata": "^0.1.13",
    "sanitize-html": "^2.11.0",
    "ts-node": "^10.9.1",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.10",
    "typeorm-extension": "^2.1.15",
    "yup": "^1.2.0",
    "zod": "^3.21.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "concurrently": "^7.5.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-perfectionist": "^1.4.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
