{
  "name": "itvault-backend",
  "version": "0.0.0",
  "description": "",
  "main": "main.js",
  "keywords": [],
  "author": "https://github.com/trolit",
  "license": "ISC",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/main.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/main.js\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/config/db.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./src/config/db.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/config/db.ts",
    "docker:db": "docker exec -it db bash"
  },
  "_moduleAliases": {
    "@enums": "dist/enums",
    "@config": "dist/config",
    "@interfaces": "dist/interfaces"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "env-var": "^7.3.0",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.2",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
